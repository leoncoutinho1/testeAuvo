@model testeAuvo.Models.Report

@{
    ViewData["Title"] = "Report";
}

<h1>Report</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Mes" class="control-label"></label>
                <select asp-for="Mes" class ="form-control" asp-items="ViewBag.Months"></select>
                <span asp-validation-for="Mes" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Ano" class="control-label"></label>
                <input asp-for="Ano" class="form-control" />
                <span asp-validation-for="Ano" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Extract" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

